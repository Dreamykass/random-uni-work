<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>MultiplicationServlet</servlet-name>
        <servlet-class>MultiplicationServlet</servlet-class>
        <init-param>
            <param-name>multiplicationParameter</param-name>
            <param-value>10</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultiplicationServlet</servlet-name>
        <url-pattern>/multiply.do</url-pattern>
    </servlet-mapping>
    <!--    <welcome-file-list>-->
    <!--        <welcome-file>multiply.do</welcome-file>-->
    <!--    </welcome-file-list>-->
</web-app>